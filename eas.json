{
  "cli": {
    "version": ">= 0.57.0"
  },
  "build": {
    "sandbox": {
      "channel": "sandbox",
      "distribution": "internal",
      "ios": {
        "image": "latest"
      },
      "android": {
        "buildType": "app-bundle",
        "image": "latest"
      },
      "env": {
        "APP_ENV": "sandbox",
        "FLIPPER_DISABLE": "1"
      },
      "prebuildCommand": "prebuild --skip-dependency-update react",
      "cache": {
        "key": "eas-1"
      }
    },
    "prod": {
      "channel": "prod",
      "distribution": "store",
      "ios": {
        "image": "latest"
      },
      "android": {
        "buildType": "app-bundle",
        "image": "latest"
      },
      "env": {
        "APP_ENV": "prod",
        "FLIPPER_DISABLE": "1"
      },
      "prebuildCommand": "prebuild --skip-dependency-update react",
      "cache": {
        "key": "eas-1"
      }
    },
    "staging": {
      "channel": "staging",
      "distribution": "internal",
      "ios": {
        "image": "latest"
      },
      "android": {
        "buildType": "apk",
        "image": "latest"
      },
      "env": {
        "APP_ENV": "staging",
        "FLIPPER_DISABLE": "1"
      },

      "prebuildCommand": "prebuild --skip-dependency-update react",
      "cache": {
        "key": "eas-1"
      }
    },
    "dev": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "image": "latest"
      },
      "android": {
        "image": "latest"
      },
      "env": {
        "APP_ENV": "dev"
      },

      "prebuildCommand": "prebuild --skip-dependency-update react",
      "cache": {
        "key": "eas-1"
      }
    },
    "simulator": {
      "ios": {
        "simulator": true,
        "image": "latest"
      },
      "android": {
        "image": "latest"
      },
      "env": {
        "APP_ENV": "dev"
      },
      "node": "14.18.1",
      "prebuildCommand": "prebuild --skip-dependency-update react",
      "cache": {
        "key": "eas-1"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "hector.dediego@distillery.com",
        "appleTeamId": "SZP43QR59Q",
        "ascAppId": "6450187176"
      },
      "android": {
        "track": "internal",
        "releaseStatus": "draft"
      }
    },
    "prod": {
      "ios": {
        "appleId": "hector.dediego@distillery.com",
        "appleTeamId": "SZP43QR59Q",
        "ascAppId": "6450187176"
      },
      "android": {
        "track": "internal",
        "releaseStatus": "draft"
      }
    },
    "sandbox": {
      "ios": {
        "appleId": "hector.dediego@distillery.com",
        "appleTeamId": "SZP43QR59Q",
        "ascAppId": "6450187176"
      },
      "android": {
        "track": "internal",
        "releaseStatus": "draft"
      }
    }
  }
}
